<%- include('partials/headmorse') %>

    <div class="container">
        <table class="striped">
            <thead>
                <tr>
                    <th>UserName</th>
                    <th>Score</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach( user => { %>
                <tr>
                    <td><%= user.userName %></td>
                    <td><%= user.score %></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </div>


    <script src="../js/main.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
   
    <script>
        M.Sidenav.init(document.querySelector('.sidenav'))
    </script>

</body>
</html>